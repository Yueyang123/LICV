{
    "files.autoSave": "on",
    "git.path":"G:/LINUXDocument/Git/bin/git",
    "workbench.iconTheme": "vscode-icons",
    "vsicons.dontShowNewVersionMessage": true,
    "c-cpp-flylint.ignoreParseErrors": true,
    "typescript.disableAutomaticTypeAcquisition": true,
    "c-cpp-flylint.clang.enable": false,
    "cppcheck.isEnable": false,
    "workbench.colorTheme": "Tomorrow Night Blue",
    "c-cpp-flylint.cppcheck.enable": false,
    "c-cpp-flylint.flexelint.enable": false,
    "window.zoomLevel": 1,
    "editor.renderWhitespace": "all",
    "editor.renderControlCharacters": true,
    "git.enableSmartCommit": true,
    "git.autofetch": true,
    "code-runner.executorMap": {
        "c": "cd $dir && gcc -g ./*.c  ./core/*.c  -L../lib/lib* -I../inc/Core_inc  -I../inc/Piclib_inc  -I../inc/Qt_inc -o ../bin/test && ../bin/test -static",
        },
        "code-runner.respectShebang": false,
        "code-runner.runInTerminal": true,
        "explorer.confirmDelete": false,
        "files.associations": {
            "gtk.h": "c",
            "JPEG.C": "cpp",
            "jpeglib.h": "c",
            "malloc.h": "c",
            "jmorecfg.h": "c",
            "cv.h": "c"
        }
}